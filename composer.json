{
  "require": {
    "craftcms/cms": "3.0.30.2",
    "vlucas/phpdotenv": "^2.4.0",
    "nystudio107/craft-seomatic": "3.1.28",
    "nystudio107/craft-imageoptimize": "1.4.43",
    "craftcms/redactor": "2.1.6"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
